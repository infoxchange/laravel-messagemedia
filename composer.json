{
  "name": "ixa-devstuff/laravel-messagemedia",
  "description": "Laravel 6+ compatible MessageMedia Messages API client with zero external dependencies (PHP 7.3+)",
  "version": "0.0.1",
  "type": "library",
  "keywords": ["laravel", "laravel-6", "messagemedia", "sms", "messages", "api", "sinch", "php73"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "IXA DevStuff",
      "email": "dev@infoxchange.org"
    }
  ],
  "homepage": "https://github.com/ixa-devstuff/laravel-messagemedia",
  "support": {
    "issues": "https://github.com/ixa-devstuff/laravel-messagemedia/issues",
    "source": "https://github.com/ixa-devstuff/laravel-messagemedia"
  },
  "require": {
    "php": ">=7.3.25",
    "laravel/framework": "~6.20.27",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.0",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "IxaDevStuff\\MessageMedia\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IxaDevStuff\\MessageMedia\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "IxaDevStuff\\MessageMedia\\ServiceProvider"
      ],
      "aliases": {
        "MessageMedia": "IxaDevStuff\\MessageMedia\\Facades\\MessageMedia"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
