{
  "name": "infoxchange/laravel-messagemedia",
  "description": "Laravel 6+ compatible MessageMedia Messages API client with zero external dependencies (PHP 7.3+)",
  "type": "library",
  "keywords": ["laravel", "laravel-6", "messagemedia", "sms", "messages", "api", "sinch", "php73", "infoxchange"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Infoxchange",
      "email": "srsdevelopers@infoxchange.org"
    }
  ],
  "homepage": "https://github.com/infoxchange/laravel-messagemedia",
  "support": {
    "issues": "https://github.com/infoxchange/laravel-messagemedia/issues",
    "source": "https://github.com/infoxchange/laravel-messagemedia"
  },
  "require": {
    "php": ">=7.3.25",
    "laravel/framework": ">=6.20.27",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.0",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Infoxchange\\MessageMedia\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Infoxchange\\MessageMedia\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Infoxchange\\MessageMedia\\ServiceProvider"
      ],
      "aliases": {
        "MessageMedia": "Infoxchange\\MessageMedia\\Facades\\MessageMedia"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
