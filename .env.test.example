# MessageMedia API Test Configuration
# Copy this file to .env.test and fill in your credentials

# MessageMedia API Credentials
MESSAGEMEDIA_API_KEY=your_api_key_here
MESSAGEMEDIA_API_SECRET=your_api_secret_here

# Test Recipient (must be a valid phone number in E.164 format)
MESSAGEMEDIA_TEST_RECIPIENT=+61491570156

# Optional: HTTP Proxy Configuration
# HTTP_PROXY=http://proxy.example.com:8080

# Optional: Test Mode (set to true to skip actual API calls)
MESSAGEMEDIA_TEST_MODE=false
